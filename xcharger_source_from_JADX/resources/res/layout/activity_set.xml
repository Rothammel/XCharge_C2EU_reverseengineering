            <FrameLayout android:id="@android:id/tabcontent" android:layout_width="match_parent" android:layout_height="match_parent">
                <LinearLayout android:orientation="horizontal" android:id="@+id/ll_apn" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="20px" android:layout_marginTop="20px" android:layout_marginRight="20px">

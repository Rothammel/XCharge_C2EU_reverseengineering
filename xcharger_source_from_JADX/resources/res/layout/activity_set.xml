                                <RadioButton android:textSize="18px" android:id="@+id/rb_wwlan_yes" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_yes"/>
                            </RadioGroup>
                        </LinearLayout>
                        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5px">
                            <TextView android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_system_cp_wait"/>
                            <RadioGroup android:orientation="horizontal" android:id="@+id/rg_cp_wait" android:layout_width="wrap_content" android:layout_height="wrap_content">
                                <RadioButton android:textSize="18px" android:id="@+id/rb_cp_wait_no" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_no"/>
                                <RadioButton android:textSize="18px" android:id="@+id/rb_cp_wait_yes" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_yes"/>
                            </RadioGroup>
                        </LinearLayout>
                        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                            <TextView android:paddingBottom="5px" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_system_background_color"/>
                            <EditText android:textSize="20px" android:id="@+id/et_background" android:focusable="false" android:focusableInTouchMode="false" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="5px" android:singleLine="true"/>
                        </LinearLayout>
                        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_monitor" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5px">
                            <TextView android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_system_monitor"/>
                            <RadioGroup android:orientation="horizontal" android:id="@+id/rg_monitor" android:layout_width="wrap_content" android:layout_height="wrap_content">
                                <RadioButton android:textSize="18px" android:id="@+id/rb_monitor_no" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_no"/>
                                <RadioButton android:textSize="18px" android:id="@+id/rb_monitor_yes" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_yes"/>
                            </RadioGroup>
                        </LinearLayout>
                        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5px">
                            <TextView android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_system_using_xcharge_logo"/>
                            <RadioGroup android:orientation="horizontal" android:id="@+id/rg_xcharge_logo" android:layout_width="wrap_content" android:layout_height="wrap_content">
                                <RadioButton android:textSize="18px" android:id="@+id/rb_xcharge_logo_no" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_no"/>
                                <RadioButton android:textSize="18px" android:id="@+id/rb_xcharge_logo_yes" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/set_yes"/>
                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </TabHost>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="200px" android:layout_marginRight="200px" android:layout_marginBottom="150px">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true">
            <EditText android:id="@+id/et_visual" android:background="@android:color/darker_gray" android:padding="10px" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            <android.inputmethodservice.KeyboardView android:id="@+id/keyboard_view" android:background="@android:color/darker_gray" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:keyTextColor="#00ff00" android:keyPreviewLayout="@layout/keyborad_preview_layout" android:keyPreviewHeight="40dp"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="24px" android:textColor="@color/white" android:id="@+id/tv_hint" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="60px" android:text="@string/set_bottom_hint" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        <Button android:textSize="18px" android:textColor="@color/white" android:id="@+id/bt_save" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="10px" android:layout_above="@+id/tv_hint" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</FrameLayout>
